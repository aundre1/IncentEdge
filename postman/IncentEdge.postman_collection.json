              "raw": "{\n  \"state\": \"NY\",\n  \"county\": \"Westchester\",\n  \"buildingType\": \"multifamily\",\n  \"totalUnits\": 300,\n  \"totalSqft\": 350000,\n  \"affordableUnits\": 90,\n  \"affordablePercentage\": 30,\n  \"totalDevelopmentCost\": 250000000,\n  \"equityInvestment\": 60000000,\n  \"projectedNOI\": 16250000,\n  \"holdPeriod\": 5,\n  \"sustainabilityTier\": \"tier_2_high_performance\",\n  \"solarCapacityKw\": 500,\n  \"entityType\": \"llc\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/calculate",
              "host": ["{{base_url}}"],
              "path": ["calculate"]
            },
            "description": "Calculate estimated incentives. No authentication required!"
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response has results\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.results).to.be.an('object');",
                  "    pm.expect(jsonData.results.incentives).to.be.an('array');",
                  "    pm.expect(jsonData.results.totals).to.be.an('object');",
                  "});",
                  "",
                  "pm.test(\"Total incentives calculated\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.results.totals.total).to.be.a('number');",
                  "    pm.expect(jsonData.results.totals.total).to.be.above(0);",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
