              ]
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Brooklyn Mixed-Use Development\",\n  \"description\": \"200-unit mixed-use building with ground floor retail\",\n  \"sector_type\": \"real-estate\",\n  \"building_type\": \"mixed_use\",\n  \"construction_type\": \"new-construction\",\n  \"address_line1\": \"123 Atlantic Avenue\",\n  \"city\": \"Brooklyn\",\n  \"state\": \"NY\",\n  \"zip_code\": \"11201\",\n  \"county\": \"Kings\",\n  \"total_sqft\": 250000,\n  \"total_units\": 200,\n  \"affordable_units\": 60,\n  \"total_development_cost\": 180000000,\n  \"hard_costs\": 140000000,\n  \"soft_costs\": 40000000,\n  \"target_certification\": \"LEED Gold\",\n  \"renewable_energy_types\": [\"solar\"],\n  \"projected_energy_reduction_pct\": 30,\n  \"domestic_content_eligible\": true,\n  \"prevailing_wage_commitment\": true,\n  \"estimated_start_date\": \"2026-06-01\",\n  \"estimated_completion_date\": \"2028-12-31\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/projects",
              "host": ["{{base_url}}"],
              "path": ["projects"]
            },
            "description": "Create a new infrastructure project"
          }
        },
        {
          "name": "Get Project by ID",
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{jwt_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/projects/{{project_id}}",
              "host": ["{{base_url}}"],
              "path": ["projects", "{{project_id}}"],
              "variable": [
                {
                  "key": "project_id",
                  "value": "mt-vernon"
                }
              ]
            },
            "description": "Get detailed project information"
          }
        }
      ],
      "description": "Manage infrastructure projects"
    },
    {
