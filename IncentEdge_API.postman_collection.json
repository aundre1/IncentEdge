{
  "info": {
    "name": "IncentEdge API",
    "_postman_id": "incentedge-api-v1",
    "description": "Complete collection for IncentEdge API - Infrastructure's Bloomberg Terminal for Incentives",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.0.0"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{jwt_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://incentedge.com/api",
      "type": "string"
    },
    {
      "key": "base_url_dev",
      "value": "http://localhost:3000/api",
      "type": "string"
    },
    {
      "key": "jwt_token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Programs",
      "item": [
        {
          "name": "Search Programs (All Federal)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs?category=federal&limit=10",
              "host": ["{{base_url}}"],
              "path": ["programs"],
              "query": [
                {
                  "key": "category",
                  "value": "federal"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
            "description": "Get all federal incentive programs"
          }
        },
        {
          "name": "Search Programs (NY State)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs?state=NY&limit=20",
              "host": ["{{base_url}}"],
              "path": ["programs"],
              "query": [
                {
                  "key": "state",
                  "value": "NY"
                },
                {
                  "key": "limit",
                  "value": "20"
                }
              ]
            },
            "description": "Get programs available in New York state"
          }
        },
        {
          "name": "Search Programs (Solar + Direct Pay)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs?technology=solar&direct_pay_eligible=true&limit=10",
              "host": ["{{base_url}}"],
              "path": ["programs"],
              "query": [
                {
                  "key": "technology",
                  "value": "solar"
                },
                {
                  "key": "direct_pay_eligible",
                  "value": "true"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
            "description": "Get solar programs with IRA direct pay eligibility"
          }
        },
        {
          "name": "Search Programs (Full Text Search)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs?search=energy efficiency&limit=20",
              "host": ["{{base_url}}"],
              "path": ["programs"],
              "query": [
                {
                  "key": "search",
                  "value": "energy efficiency"
                },
                {
                  "key": "limit",
                  "value": "20"
                }
              ]
            },
            "description": "Full-text search for 'energy efficiency' programs"
          }
        },
        {
          "name": "Get Program by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs/{{program_id}}",
              "host": ["{{base_url}}"],
              "path": ["programs", "{{program_id}}"],
              "variable": [
                {
                  "key": "program_id",
                  "value": ""
                }
              ]
            },
            "description": "Get detailed information for a specific program"
          }
        },
        {
          "name": "Get Program Statistics (HEAD)",
          "request": {
            "method": "HEAD",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs",
              "host": ["{{base_url}}"],
              "path": ["programs"]
            },
            "description": "Get program counts by category (in response headers)"
          }
        }
      ],
      "description": "Search and retrieve incentive programs"
    },
    {
      "name": "Projects",
