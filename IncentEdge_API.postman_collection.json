    {
      "name": "Example Workflows",
      "item": [
        {
          "name": "Workflow: Find NY Solar Programs",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs?state=NY&technology=solar&status=active&limit=20&sort_by=amount_max&sort_order=desc",
              "host": ["{{base_url}}"],
              "path": ["programs"],
              "query": [
                {
                  "key": "state",
                  "value": "NY"
                },
                {
                  "key": "technology",
                  "value": "solar"
                },
                {
                  "key": "status",
                  "value": "active"
                },
                {
                  "key": "limit",
                  "value": "20"
                },
                {
                  "key": "sort_by",
                  "value": "amount_max"
                },
                {
                  "key": "sort_order",
                  "value": "desc"
                }
              ]
            },
            "description": "Find all active solar programs in NY, sorted by highest value"
          }
        },
        {
          "name": "Workflow: IRA Direct Pay Programs",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/programs?category=federal&direct_pay_eligible=true&transferable=true&limit=50",
              "host": ["{{base_url}}"],
              "path": ["programs"],
              "query": [
                {
                  "key": "category",
                  "value": "federal"
                },
                {
                  "key": "direct_pay_eligible",
                  "value": "true"
                },
                {
                  "key": "transferable",
                  "value": "true"
                },
                {
                  "key": "limit",
                  "value": "50"
                }
              ]
            },
            "description": "Find IRA programs with direct pay + transferable benefits"
          }
        },
        {
          "name": "Workflow: Calculate Then Search",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Save calculated state for next request",
                  "pm.environment.set(\"calculated_state\", pm.response.json().project.state);"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"state\": \"CA\",\n  \"buildingType\": \"commercial\",\n  \"totalSqft\": 50000,\n  \"totalDevelopmentCost\": 25000000,\n  \"equityInvestment\": 7500000,\n  \"solarCapacityKw\": 250\n}"
            },
            "url": {
              "raw": "{{base_url}}/calculate",
              "host": ["{{base_url}}"],
              "path": ["calculate"]
            },
            "description": "Step 1: Calculate incentives for CA project (saves state to environment)"
          }
        }
      ],
      "description": "Common workflow examples and patterns"
    }
