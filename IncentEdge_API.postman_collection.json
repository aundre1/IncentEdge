                {
                  "key": "token",
                  "value": "{{jwt_token}}",
                  "type": "string"
                }
              ]
            },
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/projects?status=active",
              "host": ["{{base_url}}"],
              "path": ["projects"],
              "query": [
                {
                  "key": "status",
                  "value": "active"
                }
